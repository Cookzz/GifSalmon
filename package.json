{
  "name": "gif-salmon",
  "version": "2.0.0-alpha.2",
  "description": "A modern cross-platform video to gif converter",
  "main": "main.js",
  "author": "Cookzz",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "tsc main.ts && tsc preload.ts && ng build --base-href ./",
    "watch": "ng build --watch --configuration development",
    "electron-tsc": "npm run build && electron .",
    "electron": "ng build --base-href ./ && electron .",
    "build_win": "npm run build && npx @electron/packager . GifSalmon --ignore=/main.ts --ignore=/preload.ts --ignore=/.angular --ignore=/.vscode --ignore=/resources --ignore=/src/app --ignore=/src/service --ignore=/src/index.html --ignore=/src/main.ts --ignore=/src/styles.css --platform=win32 --icon=./resources/logo-hires-v2.iconset --out=./build/win/ --overwrite",
    "build_linux": "npm run build && npx @electron/packager . GifSalmon --ignore=/main.ts --ignore=/preload.ts --ignore=/.angular --ignore=/.vscode --ignore=/resources --ignore=/src/app --ignore=/src/service --ignore=/src/index.html --ignore=/src/main.ts --ignore=/src/styles.css --platform=linux --icon=./resources/logo-hires-v2-256x256@2x.png --out=./build/linux/ --overwrite",
    "build_mac": "npm run build && npx @electron/packager . GifSalmon --ignore=/main.ts --ignore=/preload.ts --ignore=/.angular --ignore=/.vscode --ignore=/resources --ignore=/src/app --ignore=/src/service --ignore=/src/index.html --ignore=/src/main.ts --ignore=/src/styles.css --platform=darwin --icon=./resources/logo-hires-v2.icns --out=./build/mac/ --overwrite",
    "build_all": "npm run build_win && npm run build_linux && npm run build_mac",
    "test": "ng test",
    "test:watch": "ng test --watch",
    "vite-init": "vitest --config ./vitest.config.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^21.0.3",
    "@angular/compiler": "^21.0.3",
    "@angular/core": "^21.0.3",
    "@angular/forms": "^21.0.3",
    "@angular/platform-browser": "^21.0.3",
    "@angular/router": "^21.0.3",
    "@primeuix/themes": "^2.0.2",
    "ffbinaries-plus": "^1.2.4",
    "fix-path": "^5.0.0",
    "jquery": "^4.0.0-rc.1",
    "lowdb": "^7.0.1",
    "primeng": "^21.0.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "which": "^6.0.0"
  },
  "devDependencies": {
    "@angular/build": "^21.0.2",
    "@angular/cli": "^21.0.2",
    "@angular/compiler-cli": "^21.0.3",
    "@electron/packager": "^19.0.1",
    "@types/jquery": "^3.5.33",
    "electron": "^39.2.6",
    "jsdom": "^27.3.0",
    "typescript": "~5.9.3",
    "vitest": "^4.0.15"
  },
  "overrides": {
    "got": "^14.5.0"
  }
}
